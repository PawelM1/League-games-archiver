<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<HBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="app.Controller.ManageController">
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets top="100.0"/>
    </padding>
    <ToggleButton minWidth="150.0" mnemonicParsing="false" onAction="#openLeagueModifyDialog" prefHeight="80.0"
                  style="-fx-background-color: #805500;" text="%Manage.toggleButton1">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
        <font>
            <Font size="18.0"/>
        </font>
    </ToggleButton>
    <ToggleButton minWidth="150.0" mnemonicParsing="false" onAction="#openSeasonModifyDialog" prefHeight="80.0"
                  style="-fx-background-color: #805500;" text="%Manage.toggleButton2">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
        <padding>
            <Insets left="40.0" right="40.0"/>
        </padding>
        <font>
            <Font size="18.0"/>
        </font>
    </ToggleButton>
    <ToggleButton minWidth="150.0" mnemonicParsing="false" onAction="#openCountryModifyDialog" prefHeight="80.0"
                  style="-fx-background-color: #805500;" text="%Manage.toggleButton3">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
        <font>
            <Font size="18.0"/>
        </font>
    </ToggleButton>
</HBox>
